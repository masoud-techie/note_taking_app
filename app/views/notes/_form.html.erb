<%= form_with model: @note do |f| %>
  <div>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </div>

  <div>
    <%= f.label :favorite %>
    <%= f.check_box :favorite %>
  </div>

  <div>
    <%= f.submit %>
  </div>
<% end %>
